{
  "TableName": "Tickets",
  "PrimaryKeyFields": "Id",
  "Fields": [
    {
      "FieldName": "Id",
      "DataType": "int",
      "NotNullable": true,
      "AutoIncrement": true,
      "Comment": "Primary key - unique ticket identifier"
    },
    {
      "FieldName": "OrganizationId",
      "DataType": "int",
      "NotNullable": true,
      "Comment": "Foreign key - reference to Organizations.Id"
    },
    {
      "FieldName": "CustomerId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to Customers.Id if associated with a customer"
    },
    {
      "FieldName": "ProjectId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to Projects.Id if associated with a project"
    },
    {
      "FieldName": "CreatedByUserId",
      "DataType": "int",
      "NotNullable": true,
      "Comment": "Foreign key - reference to Users.Id who created this ticket"
    },
    {
      "FieldName": "AssignedToUserId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to Users.Id for assigned support personnel"
    },
    {
      "FieldName": "TicketNumber",
      "DataType": "varchar(20)",
      "NotNullable": false,
      "Comment": "Human-readable ticket number (e.g., TICKET-001)"
    },
    {
      "FieldName": "Title",
      "DataType": "varchar(255)",
      "NotNullable": true,
      "Comment": "Brief description of the ticket issue"
    },
    {
      "FieldName": "Description",
      "DataType": "mediumtext",
      "NotNullable": false,
      "Comment": "Detailed description of the issue with rich text content"
    },
    {
      "FieldName": "StatusId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to TicketStatusValues.Id"
    },
    {
      "FieldName": "PriorityId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to TicketPriorityValues.Id"
    },
    {
      "FieldName": "Category",
      "DataType": "varchar(50)",
      "NotNullable": true,
      "DefaultValue": "Support",
      "Comment": "Ticket category (Support, Bug, Feature Request, etc.)"
    },
    {
      "FieldName": "CreatedAt",
      "DataType": "timestamp",
      "NotNullable": true,
      "DefaultValue": "CURRENT_TIMESTAMP",
      "Comment": "Date and time when ticket was created"
    },
    {
      "FieldName": "UpdatedAt",
      "DataType": "timestamp",
      "NotNullable": true,
      "DefaultValue": "CURRENT_TIMESTAMP",
      "Comment": "Auto-updated timestamp of last modification"
    },
    {
      "FieldName": "ResolvedAt",
      "DataType": "timestamp",
      "NotNullable": false,
      "Comment": "Date and time when ticket was resolved"
    },
    {
      "FieldName": "ClosedAt",
      "DataType": "timestamp",
      "NotNullable": false,
      "Comment": "Date and time when ticket was closed"
    },
    {
      "FieldName": "DeveloperUserId",
      "DataType": "int",
      "NotNullable": false,
      "Comment": "Foreign key - reference to Users.Id for assigned developer"
    },
    {
      "FieldName": "ScheduledDate",
      "DataType": "date",
      "NotNullable": false,
      "Comment": "Scheduled date for ticket resolution or work"
    },
    {
      "FieldName": "ExternalTicketId",
      "DataType": "varchar(100)",
      "NotNullable": false,
      "Comment": "External ticket ID from integrated systems (Jira, GitHub Issues, etc.)"
    },
    {
      "FieldName": "FirstResponseAt",
      "DataType": "timestamp",
      "NotNullable": false,
      "Comment": "Timestamp of first non-creator staff response for SLA tracking"
    }
  ],
  "Relationships": [
    {
      "Name": "TicketOrganization",
      "Description": "Ticket belongs to an Organization",
      "FromField": "OrganizationId",
      "ToTable": "Organizations",
      "ToField": "Id",
      "Type": "ManyToOne"
    },
    {
      "Name": "TicketCustomer",
      "Description": "Ticket can be associated with a Customer",
      "FromField": "CustomerId",
      "ToTable": "Customers",
      "ToField": "Id",
      "Type": "ManyToOne"
    },
    {
      "Name": "TicketProject",
      "Description": "Ticket can be associated with a Project",
      "FromField": "ProjectId",
      "ToTable": "Projects",
      "ToField": "Id",
      "Type": "ManyToOne"
    },
    {
      "Name": "TicketCreator",
      "Description": "Ticket was created by a User",
      "FromField": "CreatedByUserId",
      "ToTable": "Users",
      "ToField": "Id",
      "Type": "ManyToOne"
    },
    {
      "Name": "TicketAssignee",
      "Description": "Ticket is assigned to a User",
      "FromField": "AssignedToUserId",
      "ToTable": "Users",
      "ToField": "Id",
      "Type": "ManyToOne"
    },
    {
      "Name": "TicketTasks",
      "Description": "Ticket can have many Tasks",
      "FromField": "Id",
      "ToTable": "Tasks",
      "ToField": "TicketId",
      "Type": "OneToMany"
    }
  ]
}
